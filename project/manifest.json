{
  "manifest_version": 3,
  "name": "Vigil AI",
  "version": "1.0",
  "description": "Friendly security reminder when you are typing credit card numbers",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
      {
          "js": ["scripts/content.js"],
          "matches": ["<all_urls>"]
      }
  ]
}