{
    "manifest_version": 3,
    "name": "Vigil Ai",
    "description": "Finds all credit card numbers on the web page.",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["scripts/content.js"]
      }
    ]
  }
  